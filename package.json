{
  "name": "GameEvents",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache --experimental-debugger",
    "test": "jest",
    "check": "rnx-align-deps",
    "fix": "rnx-align-deps --write",
    "update": "rnx-align-deps --set-version",
    "install": "./android/gradlew dependencies",
    "debug": "react-native start --experimental-debugger"
  },
  "dependencies": {
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.11.0",
    "axios": "^1.7.4",
    "dayjs": "^1.11.13",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "^0.74.0",
    "react-native-flex-grid": "^1.0.4",
    "react-native-gesture-handler": "^2.19.0",
    "react-native-reanimated": "3.10.0",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.31.0",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.74.0",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "^0.74.0",
    "@react-native/typescript-config": "0.73.1",
    "@rnx-kit/align-deps": "^2.5.2",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "prettier": "^3.x.x",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.74"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "core/metro-config",
        "jest",
        "navigation/native",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens"
      ]
    }
  }
}
